      </DisplayRules>
      <Actions>
        <JavaScriptFunction FunctionName="resetDurations" Library="$webresource:ts_/js/TeamPlanningDataRibbon.js">
          <CrmParameter Value="PrimaryControl" />
        </JavaScriptFunction>
      </Actions>
    </CommandDefinition>
  </CommandDefinitions>
